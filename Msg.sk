options:
	s: "skyworld" or "survival" or "skygrid" or "nether"
	b: "defaultplot" or "playerplot" or "proplot" or "proworld" or "emporium" or "freebuild"
	p: "Escape" or "Maze" or "Spleef" or "PVP" or "DuckHunt" or "KingOfTheLadder" or "Gunfight" or "SurvivalGames" or "Parkour" or "Tardis" or "MobArena" or "Gunfight"

command /msg <player> <text>:
	aliases: /tell, /whisper
	trigger:
		if player is in world {@b}:
			if arg 1 is in world {@b}:
				send "&7[&1Me &7&m->&r &1%display name of arg 1%&7] &f%arg 2%" to player
				send "&7[&1%display name of player% &7&m->&r &1Me&7] &f%arg 2%" to arg 1
			else if arg 1 is in world {@s}:
				send "&7[&1Me &7&m->&r &b%display name of arg 1%&7] &f%arg 2%" to player
				send "&7[&1%display name of player% &7&m->&r &bMe&7] &f%arg 2%" to arg 1
			else if arg 1 is in world {@p}:
				send "&7[&1Me &7&m->&r &3%display name of player%&7] &f%arg 2%" to player
				send "&7[&1%display name of player% &7&m->&r &3Me&7] &f%arg 2%" to arg 1
