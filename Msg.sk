				send "&7[&bMe &7&m->&r &3%display name of arg 1%&7] &f%arg 2%" to player
				send "&7[&b%display name of player% &7&m->&r &3Me&7] &f%arg 2%" to arg 1
			else:
				send "&7[&bMe &7&m->&r &f%display name of arg 1%&7] &f%arg 2%" to player
				send "&7[&b%display name of player% &7&m->&r &fMe&7] &f%arg 2%" to arg 1

		else if player is in world {@p}:
			if arg 1 is in world {@b}:
				send "&7[&3Me &7&m->&r &1%display name of arg 1%&7] &f%arg 2%" to player
				send "&7[&3%display name of player% &7&m->&r &1Me&7] &f%arg 2%" to arg 1
			else if arg 1 is in world {@s}:
				send "&7[&3Me &7&m->&r &b%display name of arg 1%&7] &f%arg 2%" to player
				send "&7[&3%display name of player% &7&m->&r &bMe&7] &f%arg 2%" to arg 1
			else if arg 1 is in world {@p}:
				send "&7[&3Me &7&m->&r &3%display name of arg 1%&7] &f%arg 2%" to player
				send "&7[&3%display name of player% &7&m->&r &3Me&7] &f%arg 2%" to arg 1
			else:
				send "&7[&3Me &7&m->&r &f%display name of arg 1%&7] &f%arg 2%" to player
				send "&7[&3%display name of player% &7&m->&r &fMe&7] &f%arg 2%" to arg 1
		else:
			if arg 1 is in world {@b}:
				send "&7[&fMe &7&m->&r &1%display name of arg 1%&7] &f%arg 2%" to player
				send "&7[&f%display name of player% &7&m->&r &1Me&7] &f%arg 2%" to arg 1
			else if arg 1 is in world {@s}:
				send "&7[&fMe &7&m->&r &b%display name of arg 1%&7] &f%arg 2%" to player
				send "&7[&f%display name of player% &7&m->&r &bMe&7] &f%arg 2%" to arg 1
			else if arg 1 is in world {@p}:
				send "&7[&fMe &7&m->&r &3%display name of arg 1%&7] &f%arg 2%" to player
				send "&7[&f%display name of player% &7&m->&r &3Me&7] &f%arg 2%" to arg 1
			else:
				send "&7[&fMe &7&m->&r &f%display name of arg 1%&7] &f%arg 2%" to player
				send "&7[&f%display name of player% &7&m->&r &fMe&7] &f%arg 2%" to arg 1			
		set {Msg.%player%.LastMsged} to arg 1
		set {Msg.%arg 1%.LastMsged} to player
	
command /r <text>:
